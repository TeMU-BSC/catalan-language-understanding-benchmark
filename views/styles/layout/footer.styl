.footer
  border none
  border-radius 0px
  margin-bottom 0px
  padding 15px 0px
  height auto
  background-color: #000
  ul
    li
      a
        font-size 1em
        color #fff !important
      a:hover
        color #eee !important
  @media(min-width:768px)
    .rightNav
      float right
    .centerNav
      margin auto
      display flex
      flex-flow row-reverse nowrap
