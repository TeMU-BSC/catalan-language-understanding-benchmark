#topCover
    min-height initial
    background-image linear-gradient(to bottom, rgba(34, 34, 34, 0) 60%, rgba(34, 34, 34, 1)), url("../AINA_header.png")
    background-position right
    background-repeat no-repeat
    height 35vh
    background-size 100% auto
    #titleImg
      width 100%

@media (min-width: 300px)
    #topCover
        height 9vh

@media (min-width: 500px)
    #topCover
        height 14vh

@media (min-width: 768px)
    #topCover
        height 25vh

@media (min-width: 1200px)
    #topCover
        height 35vh
